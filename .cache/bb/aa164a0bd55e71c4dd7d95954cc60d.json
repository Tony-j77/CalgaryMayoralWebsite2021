rm: scale(1,1);\n        -webkit-transform: scale(1,1);\n        -moz-transform: scale(1,1);\n        -ms-transform: scale(1,1);\n        -o-transform: scale(1,1);\n}\n\n.active {\n    content: '';\n        position: absolute;\n        width: 100%;\n        height: 0.4rem;\n        background:#1b5ab3;\n        left: 0;\n        bottom: 0;\n  }\n\n\n    \n  /* Style the collapsible content. Note: hidden by default */\n \n\n  .collapsediv {\n    padding: 1vw; \n    height:auto;\n    margin-left: auto;\n    margin-right: auto;\n  }\n\n  .boldideas {\n    font-family: \"Decimal\", sans-serif;\n    color: #113e88;\n    text-align: center;\n    width: 92%;\n    display: block;\n    margin: 0 auto;\n    font-size: calc(30px + 20 * ((100vw - 375px) / 1065));\n    line-height: 120%;\n    letter-spacing: -3px;\n    margin: 50px;\n    font-weight: 300;\n  }\n\n  .buildback {\n      font-weight: 400;\n      font-family: \"Decimal\", sans-serif;\n      color: #113e88;\n      text-align: center;\n      margin: auto;\n      font-size: calc(20 * ((100vw - 375px) / 1065));\n      line-height: 120%;\n      margin: 50px;\n      font-weight: 300;\n  }\n\n  .content {\n    margin-left: 7vw;\n    background-color: red;\n    width: 85%;\n    max-height: 0;\n    overflow: hidden;\n    transition: max-height 0.2s ease-out;\n  }"},"lineCount":null}},"error":null,"hash":"59c6671db19d65369c688d827d4992b4","cacheData":{"env":{}}}